---
import { storyblokEditable } from '@storyblok/astro';

import type { LinkStoryblok } from '@/lib/storyblok/types';

export type LinkBlock = Props;
type Props = {
	blok: LinkStoryblok;
};

const { blok } = Astro.props;
---

<div {...storyblokEditable(blok)}>
	<p>{JSON.stringify(blok.component)}</p>
</div>
